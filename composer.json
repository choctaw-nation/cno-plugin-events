{
	"name": "choctaw-nation/choctaw-events-plugin",
	"description": "a custom events plugin for the Choctaw Country site",
	"type": "project",
	"autoload": {
		"psr-4": {
			"ChoctawNation\\Events\\": "inc/"
		}
	},
	"config": {
		"allow-plugins": {
			"composer/installers": true,
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"require-dev": {
		"wp-coding-standards/wpcs": "*"
	},
	"require": {
		"wp-graphql/wp-graphql": "^1.17"
	}
}
